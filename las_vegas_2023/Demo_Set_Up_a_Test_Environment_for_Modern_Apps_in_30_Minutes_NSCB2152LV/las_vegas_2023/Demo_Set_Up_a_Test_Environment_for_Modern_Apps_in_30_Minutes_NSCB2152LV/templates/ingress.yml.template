apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: ingress
spec:
  rules:
    - host: ingress.${clusterName}.${domain}
      http:
        paths:
          - pathType: Prefix
            path: "/"
            backend:
              service:
                name: web-front-1
                port:
                  number: 80